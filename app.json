{
  "expo": {
    "name": "swardify",
    "slug": "swardify",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.bricesuazo.swardify",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Jua-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ea7d1bbc-f1d8-4595-bb54-d7fb618bb5c2"
      }
    },
    "android": {
      "package": "com.bricesuazo.swardify"
    }
  }
}
