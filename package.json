{
  "name": "swardify",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "db:cli": "pnpm db:generate && pnpm db:dump",
    "db:generate": "supabase gen types typescript --linked --schema public > src/types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:dump": "pnpx supabase@beta db dump -f supabase/schema.sql",
    "db:seed": "pnpx supabase@beta db dump --data-only -f supabase/seed.sql"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.18.0",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "aes-js": "^3.1.2",
    "clsx": "^2.1.0",
    "expo": "^50.0.8",
    "expo-constants": "~15.4.2",
    "expo-linking": "~6.2.1",
    "expo-router": "~3.4.1",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.1",
    "expo-status-bar": "~1.11.1",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.5",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.7.2",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "~3.29.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/aes-js": "^3.1.4",
    "@types/eslint": "^8.56.5",
    "@types/react": "~18.2.45",
    "@types/react-native-get-random-values": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
