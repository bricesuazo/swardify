{
  "name": "swardify",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "run:android": "expo run:android",
    "run:ios": "expo run:ios",
    "web": "expo start --web",
    "db:cli": "pnpm db:generate && pnpm db:dump && pnpm db:diff",
    "db:generate": "supabase gen types typescript --local --schema public > src/types/supabase.ts",
    "db:dump": "supabase db dump --local -f supabase/schema.sql",
    "db:diff": "supabase db diff -f",
    "db:reset": "supabase db reset",
    "db:seed": "pnpx supabase@beta db dump --data-only -f supabase/seed.sql"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-native-community/blur": "^4.4.0",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-picker/picker": "^2.6.1",
    "@react-navigation/drawer": "^6.6.14",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.28.6",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "aes-js": "^3.1.2",
    "clsx": "^2.1.0",
    "expo": "^50.0.8",
    "expo-constants": "~15.4.2",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.1",
    "expo-router": "~3.4.1",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.1",
    "expo-status-bar": "~1.11.1",
    "lodash.debounce": "^4.0.8",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.5",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-otp-entry": "^1.6.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "~3.29.0",
    "react-native-ui-lib": "^7.17.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/aes-js": "^3.1.4",
    "@types/eslint": "^8.56.5",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "~18.2.45",
    "@types/react-native-get-random-values": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
